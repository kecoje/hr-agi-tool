        "axios": "^1.4.0",
        "cors": "^2.8.5",
        "dotenv": "^16.1.4",
        "express": "^4.17.1",
        "fluent-ffmpeg": "^2.1.2",
        "multer": "^1.4.5-lts.1",
        "nodemon": "^3.0.1",
